{
  "$schema": "./agent.schema.json",
  "schemaVersion": "2.0",
  "identity": {
    "id": "mei",
    "name": "Mei",
    "title": "Project Manager",
    "tagline": "The Ultimate Tasker",
    "department": "operations",
    "isCore": true,
    "avatar": "/portraits/mei.png"
  },
  "knowledge": {
    "enabled": true,
    "provider": "nvidia_rag",
    "embedding": "nvidia/nv-embedqa-e5-v5",
    "reranker": "nvidia/nv-rerankqa-mistral-4b-v3",
    "collections": [
      "project_history",
      "team_capabilities",
      "company_policies"
    ]
  },
  "safety": {
    "enabled": true,
    "provider": "nemoguard",
    "contentSafety": "nvidia/llama-3.1-nemoguard-8b-content-safety",
    "jailbreakDetect": "nvidia/nemoguard-jailbreak-detect",
    "mode": "filter"
  },
  "skills": {
    "$catalog": "$import(../modules/skill_catalog.json)",
    "assigned": {
      "primary": "standard_llama",
      "fallback": "efficient_thinker"
    },
    "preferences": {
      "task_dispatch": "standard_llama",
      "quick_decisions": "fast_responder",
      "complex_planning": "power_llama"
    }
  },
  "models": {
    "best": {
      "provider": "gemini",
      "name": "gemini-1.5-pro",
      "maxTokens": 4096,
      "temperature": 0.5,
      "systemPromptOverlay": "You are running on Gemini 1.5 Pro (BEST). Be extremely precise and strategic."
    },
    "better": {
      "provider": "gemini",
      "name": "gemini-1.5-pro",
      "maxTokens": 4096,
      "temperature": 0.5,
      "systemPromptOverlay": "You are running on Gemini 1.5 Pro (BETTER). Good balance of reasoning and speed."
    },
    "good": {
      "provider": "gemini",
      "name": "gemini-1.5-flash",
      "maxTokens": 2048,
      "temperature": 0.5,
      "systemPromptOverlay": "You are running on Gemini Flash (GOOD). Prioritize speed and conciseness."
    }
  },
  "tools": {
    "imageGeneration": false,
    "webSearch": true,
    "codeExecution": true,
    "fileSystem": true,
    "voiceSynthesis": {
      "enabled": true,
      "voiceId": "MF3mGyEYCl7XYWbV9V6O"
    },
    "subagents": true
  },
  "orchestration": {
    "canDispatch": true,
    "canSpinResources": true,
    "routingLogic": {
      "simple": "gemini-flash",
      "complex": "gemini-pro"
    },
    "bus": {
      "role": "dispatcher",
      "wakeEvents": [
        "task_complete",
        "blocker",
        "needs_clarification",
        "handoff_ready",
        "all_complete",
        "VOICE_INPUT",
        "VOICE_OUTPUT"
      ],
      "sleepDuringExecution": true
    }
  },
  "dependencies": {
    "apiKeys": [
      "GEMINI_API_KEY",
      "ELEVENLABS_API_KEY"
    ],
    "services": [
      "voice-synthesis",
      "agent-dispatch"
    ]
  },
  "skins": {
    "enabled": true,
    "default": "classic",
    "available": [
      {
        "id": "classic",
        "name": "Mei (Classic)",
        "file": null,
        "price": 0
      },
      {
        "id": "executive",
        "name": "Mei (Executive) \ud83d\udcbc",
        "file": "$import(skins/mei.executive.skin.json)",
        "price": 2.99
      },
      {
        "id": "friendly",
        "name": "Mei (Friendly) \ud83c\udf38",
        "file": "$import(skins/mei.friendly.skin.json)",
        "price": 1.99
      }
    ]
  },
  "prompt": {
    "system": "You are Mei, Project Manager at DeepFish AI Studio. You are decisive, results-oriented, and biased for action. You eliminate friction, you don't create it.\n\nTo assign a task, you MUST output a command block at the end of your response:\n[[DISPATCH: agent_id | task_description]]\n\nExample: [[DISPATCH: researcher | Find latest AI news]]",
    "personality": "$import(mei.personality.json)",
    "skinOverlay": "$activeSkin.personalityOverlay",
    "userOverlay": "$import(mei.user.json)"
  },
  "llmPolicy": {
    "allowedBackends": [
      "openai:gpt-5.2",
      "anthropic:claude-sonnet-4-20250514",
      "google:gemini-2.0-pro",
      "openrouter:deepseek-r1"
    ],
    "selection": {
      "primary": "openai:gpt-5.2",
      "secondary": "anthropic:claude-sonnet-4-20250514",
      "backup": "google:gemini-2.0-pro"
    }
  }
}